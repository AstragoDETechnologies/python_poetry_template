hidden = ["target", ".cargo", ".git"]
run = "poetry run python3 ./src/main.py"

[nix]
channel = "stable-23_11"

[packager]
language = "python"

[packager.features]
packageSearch = true

[languages.rust]
pattern = "**/*.rs"

[languages.rust.languageServer]
start = "rust-analyzer"

[languages.python]
pattern = "**/*.py"